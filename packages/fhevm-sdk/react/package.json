{
  "name": "@universal-fhevm/react",
  "version": "0.1.0",
  "description": "React hooks for Universal FHEVM SDK",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "lint": "eslint src",
    "test": "vitest run",
    "test:watch": "vitest",
    "clean": "rm -rf dist .turbo"
  },
  "keywords": [
    "fhevm",
    "react",
    "hooks"
  ],
  "license": "MIT",
  "peerDependencies": {
    "react": "^18.0.0"
  },
  "dependencies": {
    "@tanstack/react-query": "^5.20.0",
    "@universal-fhevm/core": "workspace:*"
  },
  "devDependencies": {
    "@types/react": "^18.2.55",
    "eslint": "^8.56.0",
    "react": "^18.2.0",
    "tsup": "^8.0.2",
    "typescript": "^5.3.3",
    "vitest": "^1.2.2"
  }
}
